{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "root/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/submit-form",
      "dest": "/api/submit-form"
    },
    {
      "src": "/(.*)",
      "dest": "/root/$1"
    }
  ]
}
